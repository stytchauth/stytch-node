{
  "name": "stytch",
  "version": "2.0.1",
  "description": "A wrapper for the Stytch API",
  "main": "dist/stytch.js",
  "types": "./index.d.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/stytchauth/stytch-node.git"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint lib",
    "test": "mocha --recursive"
  },
  "author": "Stytch",
  "bugs": {
    "url": "git://github.com/stytchauth/stytch-node/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "chai": "^3.5.0",
    "eslint": "^7.29.0",
    "mocha": "^5.2.0",
    "prettier": "2.3.1",
    "sinon": "^11.1.1",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "moment": "^2.17.1",
    "request": "^2.55.0"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "commonjs": true,
      "node": true,
      "mocha": true
    },
    "rules": {
      "indent": [
        "error",
        4
      ],
      "no-underscore-dangle": 0,
      "strict": 0,
      "prefer-rest-params": 0
    }
  }
}
